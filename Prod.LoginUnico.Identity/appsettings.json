{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\Produce_LoginUnico_Logs_.txt",
          "rollingInterval": "Day",
          "outputTemplate": "Fecha Evento: {Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}{NewLine}Tipo Evento: {Level:u}{NewLine}Clase: {SourceContext}{NewLine}Mensaje: {Message:lj}{NewLine}Detalle del evento: {Exception}{NewLine}{NewLine}",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ]
  },
  "StaticFiles": {
    //"Logo": "\\\\srvfile01\\ssa_des$\\"
    "Logo": "D:\\ssa_des$\\"
  },
  "AllowedHosts": "*"
}
