{
  "name": "prod.loginunico.web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44428 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44428 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build": "ng build",
    "build:ssr": "ng run Prod.LoginUnico.Web:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev": "set ASPNETCORE_HTTPS_PORT=7086 && ng serve --port 4200 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.6",
    "@angular/common": "^14.2.6",
    "@angular/compiler": "^14.2.6",
    "@angular/core": "^14.2.6",
    "@angular/forms": "^14.2.6",
    "@angular/platform-browser": "^14.2.6",
    "@angular/platform-browser-dynamic": "^14.2.6",
    "@angular/platform-server": "^14.2.6",
    "@angular/router": "^14.2.6",
    "@siemens/ngx-datatable": "^21.3.1",
    "bootstrap": "^5.1.3",
    "bs-stepper": "^1.7.0",
    "ng-recaptcha": "^10.0.0",
    "ngx-bootstrap": "^9.0.0",
    "ngx-cookie-service": "^14.0.1",
    "ngx-perfect-scrollbar": "^10.1.1",
    "ngx-spinner": "^14.0.0",
    "ngx-toastr": "^15.0.0",
    "qs": "^6.11.0",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.5.5",
    "sweetalert2": "^11.4.33",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular/cli": "^14.2.6",
    "@angular/compiler-cli": "^14.2.6",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.7.23",
    "@types/qs": "^6.9.7",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "typescript": "~4.7.4"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}
